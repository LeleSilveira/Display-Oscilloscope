 int i,j,k,z,yi=yres,y2=(xres+1)*(yres+1)/8,stp=y2/(yres+1);
  for(k =y2; k >= stp; k-=stp){//2*yres+1=31
      z=k;
      for(j = 0;j <=xres ; j++){
        i=j;
        if(j>=8){
           i=j-8*(int)(j/8.0);
          z=k-(int)(j/8.0) ;
        }
        byte mask = 0x01 << i;
        byte answer = (frame[y2-z] & mask) >> i;
       // Serial.println(answer);
        if (answer != 0x01) {//!=0x01
          xval[j][yi]=(round((double)j * xo / (double)xres));//dac_output_voltage(DAC_CHANNEL_1, round((float)j * 255.0 / (float)xres));
          yval[j][yi]=(round((double)yi *  yo / (double)yres));//dac_output_voltage(DAC_CHANNEL_2, round((float)yi * 255.0 / (float)yres));
           // total++;
           /*Serial.print("j:");
           Serial.println(j);
           Serial.print("yi:");
           Serial.println(yi);*/
         // delayMicroseconds(100);
        }
        else{
          xval[j][yi]=-1;
        }
      }
    yi=yi-1;
  }
}



